[
~[tlist_sql;
SELECT p1.dcid
FROM pgfinalgrades p1
LEFT JOIN students s1
ON p1.studentid = s1.id
LEFT JOIN sections s2
ON p1.sectionid = s2.id
LEFT JOIN courses c1
ON s2.course_number = c1.course_number
WHERE (p1.dcid IS NULL
~[if.~(gpv.noStudent)=1]OR s1.id IS NULL[/if]
~[if.~(gpv.noSection)=1]OR s2.id IS NULL[/if]
~[if.~(gpv.noCourse)=1]OR c1.course_number IS NULL[/if]
)~[if.is.a.school]AND s1.schoolid = ~[v.curschoolid][/if.is.a.school]
ORDER BY p1.ID
]
{
"dcid" : "~(dcid)"
},
[/tlist_sql]
{}
]