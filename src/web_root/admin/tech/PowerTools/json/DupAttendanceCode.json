{"ResultSet":[
	~[tlist_sql;
SELECT a1.att_code
, (a1.yearid + 1990)||'-'||(a1.yearid + 1991)
, CASE
  WHEN a1.schoolid = '0'
  THEN 'District Office'
  WHEN a1.schoolid IN (
    SELECT School_Number
    FROM schools)
  THEN (
    SELECT name
    FROM schools
    WHERE school_number = a1.schoolid)
  ELSE 'School Does Not Exist'
  END SchoolName
, count(*)
FROM attendance_code a1
~[if.is.a.school]WHERE a1.schoolid = ~[v.curschoolid][/if]
GROUP BY a1.att_code
, a1.yearid
, a1.schoolid
HAVING COUNT(*) > 1
ORDER BY SchoolName
, a1.yearid
, a1.att_code
  ]
{
"attendanceCode" : "~(att_code)",
"yearId" : "~(yearid)",
"schoolName" : "~(schoolid)",
"count" : "~(count)"
},
[/tlist_sql]
{}
    ]}