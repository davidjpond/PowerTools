WHERE (r1.id IS NULL
~[if.~(gpv.noStudent)=1]OR s1.id IS NULL[/if]
~[if#curschool.~(v.curschoolid)=0]
~[if#noSchool.~(gpv.noSchool)=1]OR s2.school_number IS NULL[/if#noSchool]
[/if#curschool]
)
~[if.is.a.school]AND r1.schoolid = ~[v.curschoolid][/if.is.a.school]
ORDER BY r1.ID
]
