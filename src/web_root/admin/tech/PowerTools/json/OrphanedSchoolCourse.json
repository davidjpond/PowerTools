{
  "ResultSet": [
~[tlist_sql;
SELECT sc.dcid, sc.id
, sc.courseid
        , sc.schoolid
, s.name
, CASE
WHEN c.course_number IS NULL
THEN 'Course does not exist'
ELSE c.course_number
END coursename
FROM school_course sc
LEFT OUTER JOIN courses c
ON sc.courseid = c.id
LEFT OUTER JOIN Schools s
ON sc.schoolid = s.school_number
WHERE c.course_number IS NULL
OR s.name IS NULL
~[if.district.office]
OR s.school_number = 999999
            [/if.district.office]
]
{
"dcid" : "~(c1.dcid)",
"id" : "~(c1.id)",
"courseId" : "~(s2.course_section)",
        "schoolNumber" : "~(sc.schoolid)",
"schoolName" : "~(s2.course_sectionsort)",
"courseName" : "~(t1.lastfirst)"
},
[/tlist_sql]
{}
]}