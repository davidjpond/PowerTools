{"ResultSet":[
~[tlist_sql;
SELECT s2.lastfirst
, c1.sectionid
, s1.name
, c1.termid
, c1.course_number
, c1.section_number
, c2.course_number
FROM ps.cc c1
LEFT JOIN ps.schools s1
ON c1.schoolid = s1.school_number
LEFT JOIN ps.students s2
ON c1.studentid = s2.id
LEFT JOIN ps.courses c2
ON lower(c1.course_number) = lower(c2.course_number)
WHERE EXISTS(
SELECT course_number
FROM ps.courses c2
WHERE LOWER(c2.course_number) = LOWER(c1.course_number)
AND c2.course_number != c1.course_number)
           ~[if.~[gpv:frn]>0]
		  AND s2.id = ~(id)
				  [/if]
~[if.is.a.school]AND c1.schoolid = ~[v.curschoolid][/if.is.a.school]
ORDER BY s2.lastfirst
]
{
"student" : "~(Student;json)",
"sectionId" : "~(sectionid;json)",
"schoolName" : "~(schoolid;json)",
"termId" : "~(termid;json)",
"ccCourseNumber" : "~(course_number;json)",
"ccSectionNumber" : "~(section_number;json)",
"districtCourseNumber" : "~(courses_number;json)"
},
[/tlist_sql]
{}
]}
