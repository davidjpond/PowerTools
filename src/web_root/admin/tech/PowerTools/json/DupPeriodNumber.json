{"ResultSet":[
	~[tlist_sql;
SELECT p1.period_number
, CASE
  WHEN p1.schoolid = '0'
  THEN 'District Office'
  WHEN p1.schoolid IN (
    SELECT School_Number
    FROM ps.schools)
  THEN (
    SELECT name
    FROM ps.schools
    WHERE school_number = p1.schoolid)
  ELSE 'School Does Not Exist'
  END SchoolName
, (p1.year_id + 1990)||'-'||(p1.year_id + 1991)
, count(*)
FROM ps.period p1
~[if.is.a.school]WHERE p1.schoolid = ~[v.curschoolid][/if.is.a.school]
GROUP BY p1.period_number
, p1.schoolid
, p1.year_id
HAVING COUNT(*) > 1
ORDER BY SchoolName
, p1.year_id
, p1.period_number
  ]
{
"period" : "~(period_number)",
"schoolName" : "~(schoolid)",
"yearId" : "~(yearid)",
"count" : "~(count)"
},
			[/tlist_sql]
{}
    ]}