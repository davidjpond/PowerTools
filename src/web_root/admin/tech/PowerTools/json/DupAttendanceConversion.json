{"ResultSet":[
~[tlist_sql;
SELECT a1.name
, (a1.year_id + 1990)||'-'||(a1.year_id + 1991)
, CASE
WHEN a1.schoolid = '0'
THEN 'District Office'
WHEN a1.schoolid IN (
SELECT School_Number
FROM schools)
THEN (
SELECT name
FROM schools
WHERE school_number = a1.schoolid)
ELSE 'School Does Not Exist'
END SchoolName
, count(*)
FROM attendance_conversion a1
~[if.is.a.school]WHERE a1.schoolid = ~[v.curschoolid][/if]
GROUP BY a1.name
, a1.schoolid
, a1.year_id
HAVING COUNT(*) > 1
ORDER BY SchoolName
, a1.year_id
, a1.name
]
{
"name" : "~(name)",
"yearId" : "~(yearid)",
"schoolName" : "~(schoolid)",
"count" : "~(count)"
},
[/tlist_sql]
{}
]}