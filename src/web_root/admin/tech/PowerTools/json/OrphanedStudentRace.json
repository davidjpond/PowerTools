{"ResultSet":[
	~[tlist_sql;
SELECT s1.dcid
, s1.id
, s2.lastfirst
, s1.studentid
, g1.Name
, s1.racecd
FROM studentrace s1
LEFT JOIN students s2
ON s1.studentid = s2.id
LEFT JOIN gen g1
ON s1.racecd = g1.Name
AND g1.cat = 'race'
WHERE (s2.id IS NULL
OR g1.Name IS NULL)
		   ~[if.~[gpv:frn]>0]
		  AND s2.id = ~(id)
				  [/if]
~[if.is.a.school]AND s2.schoolid = ~[v.curschoolid][/if.is.a.school]
ORDER BY s1.DCID
	]
{
"dcid" : "~(s1.dcid)",
"id" : "~(s1.id)",
"student" : "~(s2.lastfirst)",
"studentid" : "~(s1.studentid)",
"raceCode" : "~(g1.name)",
"raceCodeId" : "~(s1.racecd)"
},
			[/tlist_sql]
{}
]}