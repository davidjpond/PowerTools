[
~[tlist_sql;
SELECT h1.dcid
FROM honorroll h1
LEFT JOIN students s1
ON h1.studentid = s1.id
LEFT JOIN schools s2
ON h1.schoolid = s2.school_number
WHERE (h1.id IS NULL
~[if.~(gpv.noStudent)=1]OR s1.id IS NULL[/if]
~[if#curschool.~(v.curschoolid)=0]
~[if#noSchool.~(gpv.noSchool)=1]OR s2.school_number IS NULL[/if#noSchool]
[/if#curschool]
)
~[if.is.a.school]AND h1.schoolid = ~[v.curschoolid][/if.is.a.school]
ORDER BY h1.ID
]
{
"dcid" : "~(dcid)"
},
[/tlist_sql]
