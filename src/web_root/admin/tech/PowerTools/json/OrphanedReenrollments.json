{"ResultSet":[
~[tlist_sql;
SELECT r1.dcid
, r1.id
, s1.lastfirst
, r1.studentid
, s2.name
, r1.schoolid
, r1.grade_level
, s2.high_grade
, s2.low_grade
FROM reenrollments r1
LEFT JOIN students s1
ON r1.studentid = s1.id
LEFT JOIN schools s2
ON r1.schoolid = s2.school_number
WHERE ((r1.schoolid NOT IN (
SELECT school_number
FROM schools)
OR (r1.grade_level < s2.low_grade
AND r1.schoolid != '999999')
OR (
r1.grade_level > s2.high_grade
AND r1.schoolid != '999999')
OR (
r1.grade_level != '99'
AND r1.schoolid = '999999'))
OR (s1.id IS NULL
OR s2.id IS NULL))
           ~[if.~[gpv:frn]>0]
		  AND s1.id = ~(id)
				  [/if]
~[if.is.a.school]AND r1.schoolid = ~[v.curschoolid][/if.is.a.school]
ORDER BY r1.ID
]
{
"dcid" : "~(r1.dcid;json)",
"id" : "~(r1.id;json)",
"student" : "~(s1.lastfirst;json)",
"studentId" : "~(r1.studentid;json)",
"schoolName" : "~(s2.Schoolname;json)",
"schoolId" : "~(r1.schoolid;json)",
"gradeLevel" : "~(r1.GradeLevel;json)",
"schoolHighGrade" : "~(s2.high_grade;json)",
"schoolLowGrade" : "~(s2.low_grade;json)"
},
[/tlist_sql]
{}
]}
