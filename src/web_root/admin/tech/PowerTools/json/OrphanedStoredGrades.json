{"ResultSet":[
	~[tlist_sql;
SELECT s1.dcid
, s2.lastfirst
, s1.studentid
, CASE
  WHEN s1.sectionid = 0
  THEN 'Manually Entered Grade'
  WHEN s3.ID IS NULL
  THEN 'Section ID ' || s1.sectionid || ' does not exist'
  WHEN c1.course_number IS NULL
  THEN 'Course Number ' || s1.course_number || ' does not exist'
  ELSE s3.course_number || '.' || s3.section_number
  END course_section
, CASE
  WHEN s1.sectionid = 0
  THEN 'Manually Entered Grade'
  WHEN s3.ID IS NULL
  THEN 'Section ID ' || s1.sectionid || ' Does Not Exist'
  WHEN c1.course_number IS NULL
  THEN 'Course Number ' || s1.course_number || ' Does Not Exist'
  ELSE s3.course_number || '.' || s3.section_number
  END course_sectionsort
, s1.course_name
, s1.termid
, s1.schoolname
FROM storedgrades s1
LEFT JOIN students s2
ON s1.studentid = s2.id
LEFT JOIN sections s3
ON s1.sectionid = s3.id
LEFT JOIN courses c1
ON s3.course_number = c1.course_number
WHERE (s2.id IS NULL
OR (
  s3.id IS NULL
  AND s3.ID != 0)
OR (c1.course_number IS NULL
  AND s1.sectionid != 0))
           ~[if.~[gpv:frn]>0]
		  AND s1.studentid = ~(id)
				  [/if]
~[if.is.a.school]AND s1.schoolid = ~[v.curschoolid][/if.is.a.school]
ORDER BY s1.DCID
	]
{
"dcid" : "~(s1.dcid)",
"student" : "~(s2.lastfirst)",
"studentId" : "~(s1.studentid)",
"courseSection" : "~(s3.course_section)",
"courseSectionSort" : "~(s3.course_sectionsort)",
"courseName" : "~(s1.coursename)",
"termId" : "~(s1.termid)",
"schoolName" : "~(s1.schoolname)"
},
[/tlist_sql]
{}
]}