{"ResultSet":[
	~[tlist_sql;
SELECT s1.id
, s1.course_number section_coursenumber
, s2.name
, c1.course_number course_coursenumber
FROM ps.sections s1
LEFT JOIN ps.courses c1
ON lower(s1.course_number) = lower(c1.course_number)
LEFT JOIN ps.schools s2
ON s1.schoolid = s2.school_number
WHERE c1.course_number != s1.course_number
~[if.is.a.school]AND s1.schoolid = ~[v.curschoolid][/if.is.a.school]
ORDER BY s1.id
  ]
{
"sectionId" : "~(SectionID)",
"sectionCourseNumber" : "~(section_coursenumber)",
"schoolName" : "~(name)",
"coursesCourseNumber" : "~(courses_coursenumber)"
},
[/tlist_sql]
{}
    ]}
